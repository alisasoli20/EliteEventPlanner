<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eliteeventplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Booking"></activity>
        <activity android:name=".Mehndi" />
        <activity android:name=".Birdal" />
        <activity android:name=".Birthday" />
        <activity android:name=".Mayoon" />
        <activity android:name=".engag" />
        <activity android:name=".wedding" />
        <activity android:name=".ContactFragment" />
        <activity
            android:name=".theme"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".HelpFragment" />
        <activity android:name=".MailFragment" />
        <activity android:name=".Mail_Fragment" />
        <activity android:name=".BookFragment" />
        <activity android:name=".PhotographerFragment" />
        <activity android:name=".VenueFragment" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>